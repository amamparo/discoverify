    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1",
    "zip-folder": "^1.0.0"
  },
  "scripts": {
    "clean": "rimraf .build",
    "build": "npm run clean && webpack --mode production",
    "local:build": "API_URL=http://localhost:3001 webpack --mode development",
    "local:server": "PORT=3001 node ./.build/server",
    "local:client": "ws -d .build/client -s index.html -p 3000",
    "local:watch": "npm run local:build && (npm run local:server & npm run local:client)",
    "local": "npm run clean && npm-watch",
    "test": "jest",
    "bundle": "babel-node aws/buildEBSourceBundle.js",
    "deploy": "babel-node aws/stack.js"
  },
  "watch": {
    "local:watch": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "js,jsx,scss",
      "quiet": false
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|jpg|png|jpeg|svg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  }
}
